<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>y2score</AssemblyName>
    <RootNamespace>You2Stock</RootNamespace>
    <PackageId>$(MSBuildProjectName)</PackageId>
    <Authors>Dowol</Authors>
    <Product>$(MSBuildProjectName)</Product>
    <PackageProjectUrl>https://dowol.github.io/you2stock</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dowol/you2stock</RepositoryUrl>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageLicenseExpression>LGPL-3.0-only</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Data.SQLite" Version="1.0.116" />
    <PackageReference Include="YoutubeExplode" Version="6.2.0" />
  </ItemGroup>

</Project>
